{
  "name": "@z-cloud/pull-to-refresh",
  "version": "1.0.0",
  "private": true,
  "description": "基于React/Vue + TS开发的下拉刷新以及加载更多组件。",
  "type": "module",
  "keywords": [
    "pull-to-refresh for React",
    "pull-to-refresh for Vue",
    "pull-to-refresh",
    "load more",
    "下拉刷新",
    "加载更多",
    "pull-to-refresh for uni and Taro"
  ],
  "sideEffects": [
    "*.css"
  ],
  "homepage": "https://zyt-cloud.github.io/virtual/index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/zyt-cloud/virtual"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "types": "dist/index.d.ts",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/react/index.d.ts",
        "default": "./dist/esm/react/index.js"
      },
      "require": {
        "types": "./dist/react/index.d.ts",
        "default": "./dist/cjs/react/index.cjs"
      }
    },
    "./vue": {
      "import": {
        "types": "./dist/vue/index.d.ts",
        "default": "./dist/esm/vue/index.js"
      },
      "require": {
        "types": "./dist/vue/index.d.ts",
        "default": "./dist/cjs/vue/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build2": "vite build",
    "lint": "eslint ."
  },
  "dependencies": {
    "@z-cloud/virtual-vanilla": "workspace:^"
  },
  "devDependencies": {
    "@tarojs/components": "catalog:",
    "@tarojs/taro": "catalog:",
    "@types/react": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "vite-plugin-css-injected-by-js": "^3.5.2"
  },
  "peerDependencies": {
    "@tarojs/components": "*",
    "@tarojs/taro": "*",
    "react": ">=17.0.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "@tarojs/components": {
      "optional": true
    },
    "@tarojs/taro": {
      "optional": true
    }
  }
}
